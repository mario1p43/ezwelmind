		DELETE FROM MIND_DIAGNOSIS_TARGET_USER
		WHERE CLIENT_CD = #{clientCd}		
	</delete>
	
	<insert id="addDiagnosisTargetUser" parameterType="map">
		INSERT INTO MIND_DIAGNOSIS_TARGET_USER
		(CLIENT_CD, USER_ID)
