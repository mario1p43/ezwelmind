				,#{addr1}
				,#{addr2}
				,#{faxNum}
				,#{telNum}
				,#{menuGroup}
				,#{siteNm}
				,#{pointNm}
				,#{openDd}
				,#{calcDd}
				,#{calcType}
				,#{pointBringYn}
				,#{contractType}
				,#{areaCd}
				,#{totEmploy}
				,#{fileNm}
				,#{fileFullPath}
				,#{useType}
				,#{useService}
				,#{useChannel}
				,#{joinType}
				,#{loginType}
				,#{useYn}
