				,#{item.edTime}
				,DATE_FORMAT(NOW(), '%Y%m%d%H%i%s')
				,#{item.mgrId}
			)
		</foreach>
	</insert>
	
	<delete id="deleteSchedule2" parameterType="com.ezwel.admin.service.pCounselorMgr.dto.MindScheduleDto">
		DELETE
		FROM MIND_SCHEDULE
		WHERE USER_ID = #{userId}
		  AND CENTER_SEQ = #{centerSeq}
		  AND SCHEDULE_TYPE = #{scheduleType}
		  AND COUNSEL_CD = 0
		  AND YMD = #{ymd}
		  AND ST_TIME = #{stTime}
	</delete>
			
